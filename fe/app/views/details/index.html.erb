<h1>Listing details</h1>

<table>
  <tr>
    <th>Memory</th>
    <th>Result</th>
    <th>State</th>
    <th>Time</th>
    <th>Input</th>
    <th>Output</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @details.each do |detail| %>
  <tr>
    <td><%= detail.memory %></td>
    <td><%= detail.result_id %></td>
    <td><%= detail.state %></td>
    <td><%= detail.time %></td>
    <td><%= detail.input %></td>
    <td><%= detail.output %></td>
    <td><%= link_to 'Show', detail %></td>
    <td><%= link_to 'Edit', edit_detail_path(detail) %></td>
    <td><%= link_to 'Destroy', detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

