<table>
	<thead>
		<tr>
			<th>コンテスト</th>
			<th>開始</th>
			<th>終了</th>
			<th></th>
		</tr>
	</thead>
	<tbody id="contests">
	</tbody>
</table>

<script>
$(document).ready(function(){
	contests = getContests();
	for( var i in contests ) {
		var con = contests[i];
		var edit = "<td>"+"<a href=\"/contests/"+con.id+"/edit\">編集</a></td>";
		$('#contests').append("<tr><td><a href=\"/judge/"+con.id+"\">"+con.name+"</a></td><td>"+con.begin+"</td><td>"+con.end+"</td>"+edit+"</tr>")
	}
});
</script>
